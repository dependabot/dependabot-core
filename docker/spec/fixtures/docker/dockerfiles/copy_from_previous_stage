FROM ubuntu:17.04

RUN touch /tmp/my_file

FROM ubuntu:17.04

COPY --from=0 /tmp/my_file /tmp/my_file

COPY --from=1 /tmp/my_file /tmp/my_file

COPY --from=1234 /tmp/my_file /tmp/my_file

COPY --from=00000:0.0.1 /tmp/my_file /tmp/my_file

COPY --from=00/00:0.0.1 /tmp/my_file /tmp/my_file

COPY --from=0/00:0.0.1 /tmp/my_file /tmp/my_file
