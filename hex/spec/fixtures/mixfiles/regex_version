defmodule DependabotTest.Mixfile do
  use Mix.Project

  def project do
    [
      app: :dependabot_test,
      version: "0.1.0",
      elixir: "~> 1.5",
      start_permanent: Mix.env == :prod,
      deps: deps()
    ]
  end

  def application do
    [extra_applications: [:logger]]
  end

  defp deps do
    [
      {:wsecli, ~r//, github: "esl/wsecli", branch: "master", override: true, only: :test}
    ]
  end
end
